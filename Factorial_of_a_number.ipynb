      "source": [
        "def fact(n):\n",
        "  f=1\n",
        "  while(n>0):\n",
        "    f=f*n\n",
        "    n=n-1\n",
        "  return(f)\n",
        "\n",
        "a =  int(input(\"enter a number:\"))\n",
        "ans= fact(a)\n",
        "print(\"Factorial:\" ,ans)"
      ]
    }
